{
  "name": "home",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm with-env next dev",
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "lint:fix": "pnpm lint --fix",
    "start": "pnpm with-env next start",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e .env --",
    "translate:google": "sh src/scripts/translate-google.sh",
    "translate:deepl": "sh src/scripts/translate-deepl.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^2.0.14",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "5.0.0-alpha.132",
    "@mui/material": "^5.13.3",
    "@mui/x-date-pickers": "^6.6.0",
    "@packages/hooks": "^0.0.0",
    "@packages/ui": "^0.0.0",
    "@t3-oss/env-nextjs": "^0.3.1",
    "firebase": "^9.22.1",
    "firecms": "^2.0.3",
    "next": "^13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.42.1",
    "react-router": "^6.11.2",
    "react-router-dom": "^6.11.2",
    "zod": "^3.21.4",
    "zustand": "^4.3.2",
    "embla-carousel-react": "^7.1.0"
  },
  "devDependencies": {
    "@packages/eslint-config": "^0.1.0",
    "@packages/tailwind-config": "^0.1.0",
    "@types/node": "^18.16.5",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "dotenv-cli": "^7.2.1",
    "json-autotranslate": "^1.11.0",
    "typescript": "^5.0.4"
  }
